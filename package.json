{
  "name": "basic-crud-auth-challenge",
  "version": "1.1.0",
  "description": "Test for CI",
  "author": "Brayan Parra Perez",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app/server/Index.ts",
    "ext": "ts, js, yaml"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "npm run build:clean && npm run build:tsc && npm run build:di",
    "build:clean": "rm -r dist; exit 0",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "cpy 'src/**/*.{json,yaml}' dist/src",
    "start": "node -r tsconfig-paths/register -r ts-node/register ./dist/src/app/server/Index.js",
    "prod": "npm run build && npm run start",
    "check-types": "tsc --noemit",
    "eslint": "eslint '*/**/*.{js,ts}' --fix --max-warnings=0 --no-error-on-unmatched-pattern"
  },
  "license": "MIT",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "axios": "0.26.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cpy-cli": "^4.2.0",
    "dotenv": "10.0.0",
    "express": "^4.19.2",
    "express-promise-router": "4.1.1",
    "helmet": "5.0.2",
    "http-status": "1.5.2",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-dependency-injection": "2.7.3",
    "reflect-metadata": "^0.2.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.9.4",
    "uuid": "^9.0.1",
    "uuid-validate": "0.0.3",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "6.0.0",
    "@types/glob": "7.2.0",
    "@types/helmet": "4.0.0",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.18",
    "@types/uuid-validate": "0.0.1",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "8.9.0",
    "eslint-config-google": "0.14.0",
    "express-list-endpoints": "6.0.0",
    "morgan": "1.10.0",
    "nodemon": "^3.1.0",
    "pactum": "3.1.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "4.0.0"
  }
}